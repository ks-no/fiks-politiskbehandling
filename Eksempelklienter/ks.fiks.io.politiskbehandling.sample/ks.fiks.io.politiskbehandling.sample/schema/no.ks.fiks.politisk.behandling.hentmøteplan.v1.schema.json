{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://no.ks.fiks.politisk.behandling/HentMøteplan.v1.schema.json",
  "title": "HentMøteplan",
  "definitions": {
    "Vedlegg": {
      "type": "object",
      "properties": {
        "tittel": {
          "type": "string"
        },
        "skjermetDokument": {
          "type": "boolean"
        },
        "dokumenttype": {
          "type": "object",
          "properties": {
            "kodeverdi": {
              "type": "string"
            },
            "kodebeskrivelse": {
              "type": "string"
            }
          }
        },
        "referanseDokumentfil": {
          "type": "string"
        }
      },
      "required": [
        "tittel",
        "skjermetDokument",
        "referanseDokumentfil"
      ]
    },
    "Utvalg": {
      "type": "object",
      "properties": {
        "utvalgId": {
          "type": "string"
        },
        "utvalgNavn": {
          "type": "string"
        }
      },
      "required": [
        "utvalgId",
        "utvalgNavn"
      ]
    },
    "Behandlingsplan": {
      "type": "object",
      "properties": {
        "tilUtvalgId": {
          "type": "string"
        },
        "tilUtvalgNavn": {
          "type": "string"
        },
        "tilMøteId": {
          "type": "string"
        },
        "tilMøtedato": {
          "type": "string",
          "format": "date-time"
        },
        "rekkefølge": {
          "type": "integer"
        }
      },
      "required": [
        "tilUtvalgId",
        "tilUtvalgNavn"
      ]
    },
    "Møte": {
      "type": "object",
      "properties": {
        "møteid": {
          "type": "string"
        },
        "møtedato": {
          "type": "string",
          "format": "date-time"
        },
        "tittel": {
          "type": "string"
        }
      },
      "required": [
        "møteid"
      ]
    },
    "Votering": {
      "type": "object",
      "properties": {
        "møtedeltaker": {
          "type": "string"
        },
        "representerer": {
          "type": "string"
        },
        "stemme": {
          "type": "object",
          "properties": {
            "Ja": {
              "description": "None",
              "type": "object"
            },
            "Nei": {
              "description": "None",
              "type": "object"
            },
            "Blankt": {
              "description": "None",
              "type": "object"
            }
          },
          "required": [
            "Ja",
            "Nei",
            "Blankt"
          ]
        }
      },
      "required": [
        "møtedeltaker",
        "representerer",
        "stemme"
      ]
    }
  },
  "type": "object",
  "properties": {
    "utvalgId": {
      "type": "string"
    },
    "utvalgNavn": {
      "type": "string"
    },
    "inkluderMøter": {
      "type": "boolean"
    }
  },
  "required": [
    "inkluderMøter"
  ]
}