{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://no.fiks.ks.utvalg/ResultatMøteplan.v1.schema.json",
  "title": "ResultatMøteplan",
  "definitions": {
    "Vedlegg": {
      "type": "object",
      "properties": {
        "tittel": {
          "type": "string"
        },
        "skjermetDokument": {
          "type": "boolean"
        },
        "dokumenttype": {
          "type": "object",
          "properties": {
            "kodeverdi": {
              "type": "string"
            },
            "kodebeskrivelse": {
              "type": "string"
            }
          }
        },
        "referanseDokumentfil": {
          "type": "string"
        }
      },
      "required": [
        "tittel",
        "skjermetDokument",
        "referanseDokumentfil"
      ]
    },
    "Møte": {
      "type": "object",
      "properties": {
        "møteid": {
          "type": "string"
        },
        "møtedato": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "møteid"
      ]
    }
  },
  "type": "object",
  "properties": {
    "utvalg": {
      "type": "object",
      "properties": {
        "utvalgId": {
          "type": "string"
        },
        "utvalgNavn": {
          "type": "string"
        },
        "møte": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Møte"
          }
        }
      },
      "required": [
        "utvalgId",
        "utvalgNavn"
      ]
    }
  }
}